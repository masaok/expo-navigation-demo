{
  "expo": {
    "name": "expo-navigation-demo",
    "slug": "expo-navigation-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/demo/icondemo-1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/demo/splashdemo.png",
      "resizeMode": "contain",
      "backgroundColor": "#7201E5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/demo/adaptive-icondemo.png",
        "backgroundColor": "#7201E5"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
